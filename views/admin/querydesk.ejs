<!--common script for all pages-->
<!-- for tabel -->

  <script>
    var phoneNo = document.querySelector('#phoneno');
    phoneNo.addEventListener('keydown', (env) => {
      console.log(env.keyCode);
      if (env.keyCode === 9 || env.keyCode === 229) {
        var no = phoneNo.value;
        var xhttp = new XMLHttpRequest();
        xhttp.onreadystatechange = function () {
          if (this.readyState == 4 && this.status == 200) {
            var data = JSON.parse(this.responseText)[0];
            if (data) {
              document.querySelector('#phoneno').value = data['phone_number'];
              document.querySelector('#passwordhash').value = data['password_hash'];
              document.querySelector('#esummitid').value = data['unique_user_id'];
              document.querySelector('#email').value = data['email'];
              document.querySelector('#fullname').value = data['full_name'];
              document.querySelector('#college').value = data['college'];
              document.querySelector('#univroll').value = data['univ_roll'];
              document.querySelector('#year').value = data['year'];
              document.querySelector('#branch').value = data['branch'];
              document.querySelector('#caid').value = data['caid'];
            } else {
              alert("Participant Not Found !");
            }
            phoneNo.focus();
          }
        };
        xhttp.open("POST", "/admin/getdata/" + no, true);
        xhttp.send();
      }
    });
  </script>
