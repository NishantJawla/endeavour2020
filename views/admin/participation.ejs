                    <a href="javascript:;">
                        <i class="fa fa-cogs"></i>
                        <span>Quiz</span>
                    </a>
                    <ul class="sub">
                        <li><a href="/admin/addquestion">Add Question</a></li>
                        <li><a href="/admin/questionlist">Question List</a></li>
                        <li><a href="/admin/result">Result</a></li>
                        <li><a href="/participant/quiz">quiz</a></li>
                    </ul>
                </li>
            </ul>
        </div>
    </aside>
  <section id="main-content">
    <section class="wrapper site-min-height">
      <h3><i class="fa fa-angle-right"></i> Prticipents List</h3>
      <div class="row mt">
        <div class="col-lg-12 col-md-6">
          <div class="form-panel">
            <form class="form-inline style-form" action="/admin/participation" method="get">
              <select class="form-control text-center" name="event">
                <option>Event Name</option>
                <%for(var i=0; i<docs.length; i++){%>
                <option value="<%=docs[i]._id%>">
                  <%=docs[i].eventName%>
                </option>
                <%}%>
              </select>
              <select class="form-control" name="payStatus">
                <option>Pay Status</option>
                <option value="">ALL</option>
                <option value="1">PAID</option>
                <option value="0">NOT PAID</option>
              </select>
              <select class="form-control" name="sortBY">
                <option>Sort By</option>
                <option value="full_name">Name</option>
                <option value="college">Collage</option>
              </select>
              <button type="submit" class="btn btn-theme">Submit</button>
            </form>
            <div class="row mt">
              <div class="col-lg-12">
                <div class="adv-table">
                  <div id="hidden-table-info_wrapper" class="dataTables_wrapper form-inline" role="grid">
                    <div class="row-fluid">
                    </div>
                    <table cellpadding="0" cellspacing="0" border="0" class="display table table-bordered dataTable" id="hidden-table-info"
                      aria-describedby="hidden-table-info_info">
                      <tr>
                        <th>Name</th>
                        <th>Phone NO</th>
                        <th>Team</th>
                        <th>Collage</th>
                        <th>Email</th>
                        <th>E-summit_id</th>
                      </tr>

                      <%if(s!== 0){%>
                        <%for(var j=0; j<s; j++){%>
                        <tr>
                          <td>
                            <%= user[j].full_name %>
                          </td>
                          <td>
                            <%= user[j].phone_number %>
                          </td>
                          <td>
                            <%= participant[j].team_id %>
                          </td>
                          <td >
                            <%= user[j].college %>
                          </td>
                          <td>
                            <%= user[j].username %>
                          </td>
                          <td>
                            <%= user[j].unique_user_id %>
                          </td>
                        </tr>
                        <%}%>
                        <%}%>
                    </table>
                    <div class="row-fluid">
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>
  </section>
