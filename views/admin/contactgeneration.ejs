<section id="main-content">
  <section class="wrapper site-min-height">
    <h4><i class="fa fa-angle-right"></i> Contact Generation</h4>
      <div class="row mt">
        <div class="col-lg-12 col-md-6">
          <div class="form-panel">
            <form class="form-inline style-form" action="/admin/contact" method="get">
              <select class="form-control" name="sortBy">
                <option>Sort By</option>
                <option value="full_name">Name</option>
                <option value="college">Collage</option>
                <option value="year">Year</option>
              </select>
              <button type="submit" class="btn btn-theme">Submit</button>
            </form>
            <div class="row mt">
              <div class="col-lg-12 col-md-6">
                <table id="table" border="1" class="table table-striped">
                  <tr>
                    <th>Sr.NO</th>
                    <th>Name</th>
                    <th>Collage</th>
                    <th>Email</th>
                    <th>Phone NO</th>
                    <th>Year</th>
                  </tr>
                  <%if(docs.length !== 0){%>
                  <%for(var i=0; i<docs.length; i++){%>
                  <tr>
                    <td>
                      <%=(i+1)%>
                    </td>
                    <td>
                      <%=docs[i].full_name%>
                    </td>
                    <td>
                      <%=docs[i].college%>
                    </td>
                    <td>
                      <%=docs[i].email%>
                    </td>
                    <td>
                      <%=docs[i].phone_number%>
                    </td>
                    <td>
                      <%=docs[i].year%>
                    </td>
                  </tr>
                  <%}%>
                  <%}%>
                </table>
              </div>
            </div>
          </div>
        </div>
      </div>
  </section>
